{
  "name": "usite",
  "description": "FLYP",
  "author": "FLYP",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "scripts": {
    "start": "nf start -p 3000",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node src/electron-wait-react",
    "react-start": "react-scripts start",
    "postinstall": "electron-rebuild --force",
    "package-linux": "electron-packager . FLYP --overwrite --asar=true --platform=linux --arch=armv7l --icon=public/favicon.ico --prune=true --out=release-builds",
    "create-debian-installer": "electron-installer-debian --src release-builds/FLYP-linux-armv7l/ --arch armv7l --config debian.json",
    "pack": "build --dir",
    "dist": "npm run build && build",
    "postinstall": "install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^3.0.7",
    "electron-builder": "^20.36.2",
    "electron-packager": "^12.2.0",
    "electron-rebuild": "^1.8.2",
    "foreman": "^3.0.1"
  },
  "build": {
    "appId": "com.electron.electron-with-create-react-app",
    "win": {
      "iconUrl": "https://cdn2.iconfinder.com/data/icons/designer-skills/128/react-256.png"
    },
    "directories": {
      "buildResources": "public"
    }
  }
}
